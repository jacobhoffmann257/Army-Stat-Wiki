<div id="<%= dom_id unit %>">
  <div class="container border">
    <div class="row border">
      <div class="col border">
        <div class= "row text-left border">
          <p><%= unit.name %></p>
        </div>
        <div class="row border">
          <% unit.models.each do |model| %>
            <%= render "models/model", model: model %>
          <% end %>
        </div>

      </div>
    </div>
    <div class="row border">
      <div class="col border">
        Weapons and Abilities
      </div>
    </div>
    <div class="row border">
      <div class="col border">
        Keywords
      </div>
    </div>
  </div>
</div>
