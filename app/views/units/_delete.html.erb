<div id="<%= dom_id unit %>_fav">
  <%= form_with(url: new_fav_path, data: { turbo: false }) do %>
    <input type="integer" id="unit" name="query_unit_id" value="<%= unit.id %>">
    <input type="integer" id="user" name="query_user_id" value="<%= current_user.id %>">
    <button>Favorite</button>
  <% end %>
</div>
